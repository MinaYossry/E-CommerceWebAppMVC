@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Error";
}

@section Style
{
    <link rel="stylesheet" href="~/css/ErrorStyle.css" />
}

<div class="container container-center">
    <div class="row justify-content-center">
        <div class="col-md-5">
            <div class="error-message"><span>Oops!</span></div>
        </div>
        <div class="col-md-7">
            <div class="row ms-5">
                <div class="col-md-12">
                   <h1 class="display-1">  401</h1>
                </div>

                <div class="col-md-12">
                    <p class="message">  Unauthorized</p>
                </div>
            </div>
        </div>
    </div>
</div>
